<Window x:Class="newDemEx.Views.MosaicaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:newDemEx.Views"
        mc:Ignorable="d"
        TextElement.FontFamily="Segoe UI"
        TextElement.FontSize="30"
        Title="Мозаика" Height="450" Width="800">
    <StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Image Source="/Res/Мастер пол.png" Height="80"/>
            <TextBlock Text="Мозаика" VerticalAlignment="Center" Margin="30 0"/>
        </StackPanel>
        <Button Name="PartnerApplicationButton" Content="Заявки партнёров" Margin="250 150 250 0" Click="PartnerApplicationButton_Click"/>
    </StackPanel>
</Window>
